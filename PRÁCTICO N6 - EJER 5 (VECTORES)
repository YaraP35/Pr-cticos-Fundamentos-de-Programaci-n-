#include <iostream>
using namespace std;

bool esCapicua(float vec[], int n) {
    for (int i = 0; i < n / 2; i++) {
        if (vec[i] != vec[n - 1 - i]) {
            return false;
        }
    }
    return true;
}

int main() {
    int n;
    cout << "Ingrese cantidad de elementos: ";
    cin >> n;
    
    float vec[n];
    cout << "Ingrese los elementos:\n";
    for (int i = 0; i < n; i++) {
        cin >> vec[i];
    }
    
    if (esCapicua(vec, n)) {
        cout << "El vector es capicua\n";
    } else {
        cout << "El vector NO es capicua\n";
    }
    
    return 0;
}
