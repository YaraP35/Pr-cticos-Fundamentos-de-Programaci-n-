#include <iostream>
using namespace std;

unsigned long long FAC(int n) {
    unsigned long long f = 1;
    for (int i = 2; i <= n; ++i) f *= i;
    return f;
}

int main() {
    int n;
    cout << "Ingrese un numero: ";
    if (!(cin >> n) || n < 0) return 1;
    cout << n << "! = " << FAC(n) << endl;
    return 0;
}
