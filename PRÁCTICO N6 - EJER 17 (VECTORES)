#include <iostream>
using namespace std;

void generarFibonacci(int fib[], int n) {
    if (n >= 1) fib[0] = 0;
    if (n >= 2) fib[1] = 1;
    
    for (int i = 2; i < n; i++) {
        fib[i] = fib[i - 1] + fib[i - 2];
    }
}

int main() {
    int n;
    cout << "Cantidad de numeros fibonnaci a generar: ";
    cin >> n;
    
    if (n <= 0) {
        cout << "Debe ingresar un numero positivo.\n";
        return 1;
    }
    
    int fib[100];
    generarFibonacci(fib, n);
    
    cout << "Los primeros " << n << " numeros de Fibonacci son:\n";
    for (int i = 0; i < n; i++) {
        cout << fib[i] << " ";
    }
    cout << endl;
    
    return 0;
}
