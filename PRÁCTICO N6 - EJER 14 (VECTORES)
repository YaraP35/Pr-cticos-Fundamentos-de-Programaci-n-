#include <iostream>
using namespace std;

void ordenarAscendente(float vec[], int n) {
    for (int i = 0; i < n - 1; i++) {
        for (int j = i + 1; j < n; j++) {
            if (vec[i] > vec[j]) {
                float temp = vec[i];
                vec[i] = vec[j];
                vec[j] = temp;
            }
        }
    }
}

int main() {
    int n;
    cout << "Cantidad de elementos: ";
    cin >> n;
    
    float vec[100];
    cout << "Ingrese los elementos:\n";
    for (int i = 0; i < n; i++) {
        cin >> vec[i];
    }
    
    ordenarAscendente(vec, n);
    
    cout << "Vector ordenado ascendentemente:\n";
    for (int i = 0; i < n; i++) {
        cout << vec[i] << " ";
    }
    cout << endl;
    
    return 0;
}
