#include <stdio.h>
#include <math.h>
int main() {
    double a,b,c,disc,x1,x2;
    printf("a = "); scanf("%lf",&a);
    printf("b = "); scanf("%lf",&b);
    printf("c = "); scanf("%lf",&c);
    if(a==0){ printf("No es cuadratica\n"); return 0; }
    disc = b*b - 4*a*c;
    if(disc>0){
        x1 = (-b + sqrt(disc))/(2*a);
        x2 = (-b - sqrt(disc))/(2*a);
        printf("x1 = %.4lf\nx2 = %.4lf\n",x1,x2);
    }
    else if(disc==0) printf("x = %.4lf\n",-b/(2*a));
    else printf("No hay solucion real\n");
    return 0;
}
