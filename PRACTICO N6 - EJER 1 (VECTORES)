#include <iostream>
using namespace std;

float maximo(float vec[], int n) {
    float max = vec[0];
    for (int i = 1; i < n; i++) {
        if (vec[i] > max) {
            max = vec[i];
        }
    }
    return max;
}

int main() {
    int n;
    cout << "Ingrese cantidad de elementos: ";
    cin >> n;
    
    float vec[n];
    cout << "Ingrese los elementos:\n";
    for (int i = 0; i < n; i++) {
        cin >> vec[i];
    }
    
    cout << "Maximo: " << maximo(vec, n) << endl;
    
    return 0;
}
